{
    "name": "@datapos/datapos-presenter-default",
    "version": "0.1.1001",
    "description": "A library that implements the default presenter in accordance with the Data Positioning presenter interface.",
    "license": "MIT",
    "private": false,
    "type": "module",
    "homepage": "https://www.datapos.app",
    "repository": {
        "type": "git",
        "url": "https://github.com/data-positioning/datapos-presenter-default.git"
    },
    "bugs": {
        "url": "https://github.com/data-positioning/datapos-presenter-default/issues"
    },
    "module": "./dist/datapos-presenter-default.es.js",
    "types": "./dist/types/src/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/datapos-presenter-default.es.js",
            "types": "./dist/types/src/index.d.ts"
        }
    },
    "files": [
        "dist"
    ],
    "dependencies": {
        "@datapos/datapos-shared": "^0.3.248",
        "@datapos/datapos-tool-highcharts": "^0.0.47",
        "@datapos/datapos-tool-micromark": "^0.1.954"
    },
    "devDependencies": {
        "@datapos/datapos-development": "^0.3.94",
        "@types/node": "^24.10.1",
        "@typescript-eslint/eslint-plugin": "^8.47.0",
        "@typescript-eslint/parser": "^8.47.0",
        "eslint": "^9.39.1",
        "eslint-plugin-import": "^2.32.0",
        "front-matter": "^4.0.2",
        "jiti": "^2.6.1",
        "license-report": "^6.8.1",
        "license-report-check": "^0.1.2",
        "nanoid": "^5.1.6",
        "npm-check-updates": "^19.1.2",
        "prettier": "^3.6.2",
        "retire": "^5.3.0",
        "rollup-plugin-visualizer": "^6.0.5",
        "typescript": "^5.9.3",
        "vite": "^7.2.4",
        "vite-plugin-dts": "^4.5.4"
    },
    "scripts": {
        "audit": "npm audit",
        "prebuild": "tsc -p tsconfig.scripts.json && node dist-scripts/prebuild.js",
        "build": "vite build",
        "bump:version": "node -e \"import('@datapos/datapos-development').then(m => m.bumpVersion())\"",
        "check": "npm outdated; npm-check-updates -i && retire",
        "document": "npm run _document:licenceReport && npm run _document:licenceCheck",
        "format": "prettier --write src/",
        "lint": "eslint .",
        "publish:toNPM": "npm publish --access public",
        "release": "npm run bump:version && npm run _build:config && npm run build && npm run _sync:withGitHub && npm run _upload && npm run publish:toNPM",
        "sync": "npm run bump:version && npm run _build:config && node -e \"import('@datapos/datapos-development').then(m => m.syncWithGitHub())\"",
        "test": "node -e \"import('@datapos/datapos-development').then(m => m.echoScriptNotImplemented('Test'))\"",
        "update:dataPosDeps": "npm run _update:developDep && npm run _update:sharedDep && npm run _update:toolHighchartsDep && npm run _update:toolMicromarkDep",
        "_build:config": "node -e \"import('@datapos/datapos-development').then(m => m.buildConfig())\"",
        "_document:licenceReport": "license-report --only=prod,peer > LICENSES.json",
        "_document:licenceCheck": "license-report-check --source ./LICENSES.json --allowed 'MIT' --allowed 'n/a' --allowed 'Apache-2.0' --output=table",
        "_sync:withGitHub": "node -e \"import('@datapos/datapos-development').then(m => m.syncWithGitHub())\"",
        "_update:developDep": "npm install --save-dev @datapos/datapos-development@latest",
        "_update:sharedDep": "npm install @datapos/datapos-shared@latest",
        "_update:toolHighchartsDep": "npm install  @datapos/datapos-tool-highcharts@latest",
        "_update:toolMicromarkDep": "npm install @datapos/datapos-tool-micromark@latest",
        "_upload": "npm run _upload:moduleToR2 && npm run _upload:moduleConfigToDO",
        "_upload:moduleConfigToDO": "node -e \"import('@datapos/datapos-development').then(m => m.uploadModuleConfigToDO())\"",
        "_upload:moduleToR2": "node -e \"import('@datapos/datapos-development').then(m => m.uploadModuleToR2('datapos-engine-eu/presenters/default'))\""
    }
}