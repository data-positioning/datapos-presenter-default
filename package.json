{
    "name": "datapos-presenter-default",
    "license": "MIT",
    "private": true,
    "type": "module",
    "version": "0.1.897",
    "dependencies": {
        "chart.js": "^4.5.1"
    },
    "peerDependencies": {
        "highcharts": "^12.4.0"
    },
    "devDependencies": {
        "@datapos/datapos-development": "^0.3.89",
        "@datapos/datapos-shared": "^0.3.210",
        "@datapos/datapos-tool-highcharts": "^0.0.3",
        "@datapos/datapos-tool-micromark": "^0.1.885",
        "@types/node": "^24.10.1",
        "@typescript-eslint/eslint-plugin": "^8.46.4",
        "@typescript-eslint/parser": "^8.46.4",
        "eslint": "^9.39.1",
        "eslint-plugin-import": "^2.32.0",
        "front-matter": "^4.0.2",
        "jiti": "^2.6.1",
        "license-report": "^6.8.1",
        "license-report-check": "^0.1.2",
        "nanoid": "^5.1.6",
        "npm-check-updates": "^19.1.2",
        "prettier": "^3.6.2",
        "retire": "^5.3.0",
        "typescript": "^5.9.3",
        "vite": "^7.2.2",
        "vite-plugin-dts": "^4.5.4"
    },
    "scripts": {
        "audit": "npm audit",
        "prebuild": "tsc -p tsconfig.scripts.json && node dist-scripts/prebuild.js",
        "build": "vite build",
        "buildConfig": "node -e \"import('@datapos/datapos-development').then(m => m.buildConfig())\"",
        "bump:version": "node -e \"import('@datapos/datapos-development').then(m => m.bumpVersion())\"",
        "check": "npm outdated; npm-check-updates -i && retire",
        "document": "npm run _document:licenceReport && npm run _document:licenceCheck",
        "format": "prettier --write src/",
        "lint": "eslint .",
        "publish:toNPM": "node -e \"import('@datapos/datapos-development').then(m => m.echoScriptNotImplemented('Publish'))\"",
        "release": "npm run sync:withGitHub && npm run build && npm run _upload:moduleToR2 && npm run _upload:moduleConfigToDO",
        "sync:withGitHub": "npm run bump:version && npm run buildConfig && node -e \"import('@datapos/datapos-development').then(m => m.syncWithGitHub())\"",
        "test": "node -e \"import('@datapos/datapos-development').then(m => m.echoScriptNotImplemented('Test'))\"",
        "update:dataPosDeps": "npm run _update:developDeps && npm run _update:sharedDeps",
        "_document:licenceReport": "license-report --only=prod,peer > LICENSES.json",
        "_document:licenceCheck": "license-report-check --source ./LICENSES.json --allowed 'MIT' --allowed 'n/a' --allowed 'Apache-2.0' --output=table",
        "_update:developDeps": "npm install --save-dev @datapos/datapos-development@latest",
        "_update:sharedDeps": "npm install --save-dev @datapos/datapos-shared@latest",
        "_upload:moduleConfigToDO": "node -e \"import('@datapos/datapos-development').then(m => m.uploadModuleConfigToDO())\"",
        "_upload:moduleToR2": "node -e \"import('@datapos/datapos-development').then(m => m.uploadModuleToR2('datapos-engine-eu/presenters'))\""
    }
}