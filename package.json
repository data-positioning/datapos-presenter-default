{
    "name": "datapos-presenter-default",
    "license": "MIT",
    "private": true,
    "type": "module",
    "version": "0.1.528",
    "peerDependencies": {
        "@datapos/datapos-shared": "^0.3.27"
    },
    "dependencies": {
        "highcharts": "^11.4.3"
    },
    "devDependencies": {
        "@datapos/datapos-development": "^0.3.18",
        "@eslint/js": "^9.35.0",
        "@types/node": "^20.14.2",
        "eslint": "^8.57.0",
        "globals": "^16.4.0",
        "prettier": "^3.3.1",
        "typescript": "^5.8.3",
        "typescript-eslint": "^8.44.0",
        "vite": "^7.1.5",
        "vite-plugin-dts": "^4.5.4",
        "wrangler": "^4.37.0"
    },
    "scripts": {
        "audit": "npm audit",
        "build": "npx vite build",
        "buildConfig": "node -e \"import('@datapos/datapos-development').then(m => m.buildConfig())\"",
        "bumpVersion": "node -e \"import('@datapos/datapos-development').then(m => m.bumpVersion())\"",
        "check": "npx npm-check-updates && npm outdated",
        "compile": "node -e \"require('@datapos/datapos-operations/scriptHelpers').compilePresenter()\"",
        "document": "npx npm-license-crawler --onlyDirectDependencies --production --relativeLicensePath --json LICENSES.json",
        "format": "prettier --write *.ts",
        "lint": "npx eslint **/*.ts",
        "publishToNPM": "echo \"***** PUBLISH TO NPM SCRIPT NOT IMPLEMENTED. *****\"",
        "release": "npm run syncWithGitHub && npm run build && npm run uploadPlugin",
        "syncWithGitHub": "npm run bumpVersion && npm run buildConfig && node -e \"import('@datapos/datapos-development').then(m => m.syncWithGitHub())\"",
        "test": "echo \"***** TEST SCRIPT NOT IMPLEMENTED. *****\"",
        "updateDependencies": "npm run updateDevelopment && npm run updateShared",
        "updateDevelopment": "npm install --save-dev @datapos/datapos-development@latest",
        "updateShared": "npm install @datapos/datapos-shared@latest",
        "uploadPlugin": "node -e \"require('@datapos/datapos-operations/scriptHelpers').uploadPlugin()\""
    }
}