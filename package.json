{
    "name": "datapos-presenter-default",
    "license": "MIT",
    "private": true,
    "type": "module",
    "version": "0.1.714",
    "devDependencies": {
        "@datapos/datapos-development": "^0.3.29",
        "@datapos/datapos-shared": "^0.3.100",
        "@types/markdown-it": "^14.1.2",
        "@types/node": "^24.10.0",
        "@typescript-eslint/eslint-plugin": "^8.46.3",
        "@typescript-eslint/parser": "^8.46.3",
        "eslint": "^9.39.1",
        "eslint-plugin-import": "^2.32.0",
        "front-matter": "^4.0.2",
        "highcharts": "^12.4.0",
        "jiti": "^2.6.1",
        "license-report": "^6.8.1",
        "license-report-check": "^0.1.2",
        "markdown-it": "^14.1.0",
        "npm-check-updates": "^19.1.2",
        "prettier": "^3.6.2",
        "retire": "^5.3.0",
        "typescript": "^5.9.3",
        "vite": "^7.1.12",
        "vite-plugin-dts": "^4.5.4"
    },
    "scripts": {
        "audit": "npm audit",
        "prebuild": "tsc -p tsconfig.scripts.json && node dist-scripts/prebuild.js",
        "build": "npx vite build",
        "buildConfig": "node -e \"import('@datapos/datapos-development').then(m => m.buildConfig())\"",
        "buildPresentersIndex": "node -e \"import('@datapos/datapos-development').then(m => m.buildPublicDirectoryIndex('presentations'))\"",
        "bumpVersion": "node -e \"import('@datapos/datapos-development').then(m => m.bumpVersion())\"",
        "check": "npm outdated; npm-check-updates -i && retire",
        "compile": "node -e \"require('@datapos/datapos-operations/scriptHelpers').compilePresenter()\"",
        "document": "npx npm-license-crawler --onlyDirectDependencies --production --relativeLicensePath --json LICENSES.json",
        "format": "prettier --write *.ts",
        "lint": "npx eslint **/*.ts",
        "publishToNPM": "echo \"***** PUBLISH TO NPM SCRIPT NOT IMPLEMENTED. *****\"",
        "release": "npm run syncWithGitHub && npm run build && npm run uploadModuleToR2 && npm run uploadModuleConfig",
        "syncWithGitHub": "npm run bumpVersion && npm run buildConfig && node -e \"import('@datapos/datapos-development').then(m => m.syncWithGitHub())\"",
        "test": "echo \"***** TEST SCRIPT NOT IMPLEMENTED. *****\"",
        "updateDependencies": "npm run updateDevelopment && npm run updateShared",
        "updateDevelopment": "npm install --save-dev @datapos/datapos-development@latest",
        "updateShared": "npm install @datapos/datapos-shared@latest",
        "uploadModuleConfig": "node -e \"import('@datapos/datapos-development').then(m => m.uploadModuleConfig())\"",
        "uploadModuleToR2": "node -e \"import('@datapos/datapos-development').then(m => m.uploadModuleToR2('datapos-presenter-default.es.js', 'datapos-engine-eu/presenters/datapos-presenter-default.es.js'))\""
    }
}